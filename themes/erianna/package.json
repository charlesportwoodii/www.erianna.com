{
  "name": "hugo-erianna-theme",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Hugo theme for www.erianna.com",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --color --config ./webpack.config.js",
    "build": "webpack -p --verbose --profile --config ./webpack.config.js --color --display-error-details; yarn run sri;",
    "sri": "echo \"---\njs: $(shasum -b -a 384 ./static/js/*.js | awk '{ print $1 }' | xxd -r -p | base64)\ncss: $(shasum -b -a 384 ./static/css/*.css | awk '{ print $1 }' | xxd -r -p | base64)\n\" > ./data/assets/main/sri.yml"
  },
  "author": "Charles R. Portwood II <charlesportwoodii@erianna.com>",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime-corejs3": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^2.0.0",
    "highlight.js": "^9.13.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "import-loader": "^1.0.1",
    "imports-loader": "^0.8.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.12.0",

    "mini-css-extract-plugin": "^0.6.0",

    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "raw-loader": "^2.0.0",
    "resolve-url": "^0.2.1",
    "sass-loader": "^7.1.0",
    "source-list-map": "2.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "3.3.1",
    "webpack-event-plugin": "^1.1.1",
    "webpack-visualizer-plugin": "^0.1.11",
    "whatwg-fetch": "^3.0.0"
  }
}
