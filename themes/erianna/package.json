{
  "name": "hugo-erianna-theme",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Hugo theme for www.erianna.com",
  "private": true,
  "scripts": {
    "webpack-dev-server": "webpack-dev-server --host 0.0.0.0 --port 8080 --public 127.0.0.1 --config webpack.config.local.js",
    "sri": "echo \"---\njs: $(shasum -b -a 384 ./static/js/*.js | xxd -r -p | base64)\ncss: $(shasum -b -a 384 ./static/css/*.css | xxd -r -p | base64)\n\" > ./data/assets/main/sri.yml",
    "watch": "webpack --progress --colors --watch",
    "build": "webpack --display-error-details; yarn run sri;"
  },
  "author": "Charles R. Portwood II <charlesportwoodii@erianna.com>",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "autoprefixer": "^7.1.2",
    "babel-core": "^v6.26.0",
    "babel-loader": "^7.1",
    "babel-preset-es2015": "v6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.10.1",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "imports-loader": "^0.6.5",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "resolve-url": "^0.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "whatwg-fetch": "^2.0.2"
  },
  "dependencies": {
    "highlight.js": "^9.12.0"
  }
}
